
<%- include ("header") -%>

    <div class="box" id="heading">
      <h1><%= whatTitle %></h1>
    </div>

    <div class="box">
      <% newInput.forEach(item => { %>
        <div class="item">
          <input type="checkbox" name="done">
          <p><%=item%></p></div>
    <%  }) %>




    <form class= "item" action="/" method="post">
      <input type="text" name="toDoInput" placeholder="type hier" autocomplete="off">
      <button type="submit" name="list" value=<%= whatTitle %>>+</button>
      </form>
</div>

<%- include ("footer") -%>
